{
    "name": "webcraftdg/fractal-cms-import-export",
    "description": "Import / Export engine for FractalCMS (CSV, XLSX, SQL, Row Transformers)",
    "keywords": [
        "import/export",
        "import",
        "export",
        "php",
        "yii2",
        "module"
    ],
    "version": "v1.0.0",
    "homepage": "https://github.com/webcraftdg/fractal-cms-import-export",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/webcraftdg/fractal-cms-import-export/issues"
    },
    "authors": [
        {
            "name": "David Ghyse",
            "email": "davidg@webcraftdg.fr"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-intl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-fileinfo": "*",
        "yiisoft/yii2": "^2.0",
        "webcraftdg/fractal-cms-core": "~v2.1.0",
        "phpoffice/phpspreadsheet": "^5.3"
    },
    "require-dev": {
        "codeception/codeception": "^5.3",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*",
        "codeception/module-yii2": "^2.0",
        "codeception/module-db": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "fractalCms\\importExport\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": []
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
