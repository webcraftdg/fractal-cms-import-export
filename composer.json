{
    "name": "webcraftdg/yii2-advanced-import-export",
    "description": "Import-Export module d'aide pour les import et export Excel et CVS",
    "keywords": [
        "import/export",
        "import",
        "export",
        "php",
        "yii2",
        "module"
    ],
    "version": "v1.0.0",
    "homepage": "https://github.com/webcraftdg/yii2-advanced-import-export",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/webcraftdg/yii2-advanced-import-export/issues"
    },
    "authors": [
        {
            "name": "David Ghyse",
            "email": "davidg@webcraftdg.fr"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-intl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-fileinfo": "*",
        "vlucas/phpdotenv": "~5.0",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-redis": "~2.0",
        "bower-asset/bootstrap": "~5.0",
        "league/flysystem": "^3.25",
        "webcraftdg/fractal-cms-core": "^2.0",
        "phpoffice/phpspreadsheet": "^5.3"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0",
        "yiisoft/yii2-gii": "~2.0",
        "codeception/codeception": "^5.3",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*",
        "codeception/module-yii2": "^2.0",
        "codeception/module-db": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "fractalCms\\importExport\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": []
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
